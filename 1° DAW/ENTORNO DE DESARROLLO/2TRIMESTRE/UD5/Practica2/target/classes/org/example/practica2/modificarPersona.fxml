    <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="306.0" prefWidth="244.0" spacing="20.0">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
        <children>
            <Label text="Nombre" />
            <TextField fx:id="inp_modificar_nombre" />
            <Label text="Apellidos" />
            <TextField fx:id="inp_modificar_apellidos" />
            <Label text="Edad" />
            <TextField fx:id="inp_modificar_edad" />
            <Button onAction="#onHandleGuardarCambios" text="Actualizar" />
            <Button onAction="#cerrarModal" text="Salir" />
        </children>
    </VBox>
</children>
</HBox>
