<HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="347.0" prefWidth="319.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.practica2.ModificarPersonaController">
